@model PMS.Areas.Identity.Data.ApplicationUser

<h2>Resubmit Documents</h2>

<form asp-action="ResubmitDocuments" method="post">
    <div class="form-group">
        <label>PAN Number</label>
        <input type="text" class="form-control" asp-for="PAN" required />
    </div>

    <div class="form-group">
        <label>Aadhar Number</label>
        <input type="text" class="form-control" asp-for="Aadhar" required />
    </div>

    <button type="submit" class="btn btn-success">Submit</button>
</form>
